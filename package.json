    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds --icon=./icon.icns --overwrite && electron-packager ./ --out=./builds --platform=win32 --arch=x64 --arch=ia32 --icon=./icon.ico --overwrite"
